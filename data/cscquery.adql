SELECT DISTINCT m.name,o.obsid,o.region_id,o.theta,m.ra,m.dec,m.significance,m.likelihood,o.src_area_b,o.flux_aper_b,o.hard_hm,o.hard_hs,o.hard_ms,o.powlaw_gamma,o.powlaw_gamma_lolim, o.powlaw_gamma_hilim, o.bb_kt, o.bb_kt_lolim, o.bb_kt_hilim, o.var_prob_b,o.var_sigma_b,o.var_mean_b,o.var_min_b,o.var_max_b,o.var_prob_h,o.var_sigma_h,o.var_mean_h,o.var_min_h,o.var_max_h,o.var_prob_m,o.var_sigma_m,o.var_mean_m,o.var_min_m,o.var_max_m,o.var_prob_s,o.var_sigma_s,o.var_mean_s,o.var_min_s,o.var_max_s,o.ks_prob_b,o.ks_prob_h,o.ks_prob_m,o.ks_prob_s,o.kp_prob_b,o.kp_prob_h,o.kp_prob_m,o.kp_prob_s,o.gti_start,o.gti_stop,o.gti_elapse FROM master_source m , master_stack_assoc a , observation_source o , stack_observation_assoc b , stack_source s WHERE ((o.flux_significance_b > 5) AND (o.powlaw_gamma IS NOT NULL) AND (o.bb_kt IS NOT NULL)) AND (m.name = a.name) AND (s.detect_stack_id = a.detect_stack_id and s.region_id = a.region_id) AND (s.detect_stack_id = b.detect_stack_id and s.region_id = b.region_id) AND (o.obsid = b.obsid and o.obi = b.obi and o.region_id = b.region_id)ORDER BY name ASC
